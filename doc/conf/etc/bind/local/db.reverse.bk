;
; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	dns.conbin.autoupb.co. ( ; mname
	admin.conbin.autoupb.co.	; rname (email)
	4				; serial
	604800				; refresh
	86400				; retry
	2419200				; expire
	604800 )			; negative cache ttl
;
@	IN	NS	dns.conbin.autoupb.co.
; @	IN	NS	dns2.conbin.autoupb.co.
dns	IN	PTR	dns.conbin.autoupb.co.
; dns2	IN	PTR	192.168.80.2
; mail	IN	A	192.168.80.2
; mx	IN	A	192.168.80.2
; www	IN	A	192.168.80.2
; wev	IN	CNAME	www
